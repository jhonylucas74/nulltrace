{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main", "*"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "core:window:allow-set-fullscreen",
    "core:window:allow-maximize",
    "core:window:allow-destroy"
  ]
}
